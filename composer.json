{
    "name": "cct-marketing/cct-technology-sdk",
    "description": "Sdk to communicate with the CCT Technology api",
    "type": "library",
    "require": {
        "php": "~8.1",
        "beberlei/assert": "^2.6|^3.0",
        "ramsey/uuid": "^3.8|^4.0",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "softonic/guzzle-oauth2-middleware": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.13",
        "vimeo/psalm": "^5.4.0",
        "nyholm/psr7": "^1.4",
        "symfony/cache": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "CCT\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCT\\SDK\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Schweppe",
            "email": "ps@cct-marketing.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    }
}
